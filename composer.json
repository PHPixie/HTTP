{
    "name": "phpixie/http",
    "description": "HTTP library for PHPixie, implements the PSR-7 standard",
    "keywords": ["http", "psr-7"],
    "homepage": "http://phpixie.com",
    "type": "library",
    "license": "BSD",
    "authors": [
        {
            "name": "Roman Tsiupa",
            "email": "draconyster@gmail.com",
            "homepage": "http://dracony.org"
        }
    ],
    "require": {
        "phpixie/slice": "~3.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "phpixie/test": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "PHPixie\\": "src/PHPixie",
            "PHPixie\\Tests\\": "tests/PHPixie/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
